{
  "Wopi": {
    "StorageProvider": {
      "RootPath": "../WopiHost/wwwroot/wopi-docs"
    },
    "HostUrl": "http://host.docker.internal:5001",
    "ClientUrl": "http://localhost:9980",
    "Discovery": {
      "NetZone": "ExternalHttp",
      "RefreshInterval": "12:00:00"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Console": {
      "IncludeScopes": false
    }
  }
}